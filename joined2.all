def zoom_effect(bg_file, fg_file):
    bg = Image.open(bg_file)
    fg = Image.open(fg_file)
    fg_copy = fg.copy()
    fg_copy = fg_copy.resize((int(fg_copy.width/2), int(fg_copy.height/2)))
    fg_copy.putalpha(0)
    result_images = []
